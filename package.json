{
  "name": "coordinator",
  "description": "A visual interface to turn an SVG into XY coordinates.",
  "author": "Aliza Aufrichtig <aliza@spotify.com>",
  "version": "0.0.1",
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.16.0",
    "eslint-plugin-html": "^4.0.1",
    "jest": "^22.4.3",
    "pre-commit": "^1.2.2",
    "rollup": "^0.52.1",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-svelte": "^3.2.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "uglify-es": "^3.2.1"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint src/*.js src/**/*.js",
    "test": "jest"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "codemirror": "^5.32.0",
    "d3-array": "^1.2.1",
    "pathologist": "^0.1.9",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "xml-beautifier": "^0.3.0"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/spotify/coordinator"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/js/tests/_setup"
    ],
    "verbose": true,
    "modulePaths": [
      "<rootDir>/src/js/tests"
    ]
  },
  "pre-commit": [
    "test",
    "lint"
  ]
}
